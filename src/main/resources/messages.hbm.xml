<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="www.project.bean.Messages" table="messages">
		<id column="mes_id" name="mes_id" type="java.lang.Integer" />
		<property column="sender" name="sender" type="java.lang.Integer" />
		<property column="receiver" name="receiver" type="java.lang.Integer" />
		<property column="msg" name="msg" type="java.lang.String" />
		<property column="timestamp" name="timestamp" type="java.sql.Timestamp" />
	</class>
</hibernate-mapping>